{
  "name": "@descript/web-audio-js",
  "description": "Pure JS implementation of the Web Audio API",
  "version": "0.14.0",
  "author": "Nao Yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/web-audio-engine/issues"
  },
  "dependencies": {
    "audio-type": "^1.0.2",
    "biquad-coeffs-webaudio": "^1.2.0",
    "fourier-transform": "1.1.2",
    "nmap": "^1.1.0",
    "scijs-window-functions": "^2.0.2",
    "wav-decoder": "1.3.0",
    "wav-encoder": "1.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rollup": "^2.3.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "sinon": "9.0.1",
    "ts-jest": "^25.3.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "files": [
    "package.json",
    "README.md",
    "build",
    "lib"
  ],
  "homepage": "https://github.com/descriptinc/web-audio-engine/",
  "keywords": [
    "web audio api"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/descriptinc/web-audio-engine.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:browser": "rollup -c",
    "build:to5": "tsc",
    "clean": "rm -rf lib coverage .nyc_output npm-debug.log",
    "cover": "BABEL_ENV=cover nyc --reporter text --reporter html npm test",
    "lint": "eslint src test",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm-run-all clean lint test build",
    "preversion": "npm-run-all clean lint test",
    "test": "jest --passWithNoTests",
    "travis": "npm-run-all lint test",
    "version": "npm run build && git add -A build"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["test/**/*.js"],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  }
}
